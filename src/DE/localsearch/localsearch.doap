         xmlns="http://usefulinc.com/ns/doap#">

  <name xml:lang="en">LocalSearch</name>
  <shortdesc xml:lang="en">Framework for filesystem local search and metadata management</shortdesc>
  <description>LocalSearch is the file search framework of the GNOME desktop. It stores
data about user files structured by the Nepomuk definitions, features a tightly sandboxed
metadata extractor, and provides facilities to alter file metadata.</description>
  <homepage rdf:resource="https://gnome.pages.gitlab.gnome.org/localsearch/" />
  <developer-forum rdf:resource="https://discourse.gnome.org/tag/tracker" />
  <download-page rdf:resource="http://download.gnome.org/sources/localsearch/" />
  <bug-database rdf:resource="https://gitlab.gnome.org/GNOME/localsearch/issues/" />
  <category rdf:resource="http://api.gnome.org/doap-extensions#core" />
  <programming-language>C</programming-language>

