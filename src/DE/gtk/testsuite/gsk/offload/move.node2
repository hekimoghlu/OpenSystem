container {
  color {
    bounds: 0 0 800 600;
    color: red;
  }
  transform {
    transform: translate(40,40);
    child: subsurface {
      child: texture {
        bounds: 0 0 20 20;
        texture: memory {
          format: RGB8;
          width: 16;
          height: 16;
          offset: 0;
          stride: 48;
          data: url("data:application/gzip;base64,H4sIAAAAAAACA2NgGAWjYOQCABX0oPUAAwAA");
        }
      }
    }
  }
}
