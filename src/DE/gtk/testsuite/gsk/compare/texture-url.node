/* Add a color node to blow up the bounds so that
   we can test the texture bounds work. */
color {
  bounds: 0 0 50 50;
  color: white;
}

texture {
  bounds: 10 10 30 30;
  texture: memory {
    format: BGRA8p;
    width: 10;
    height: 10;
    offset: 0;
    stride: 40;
    data: url("data:application/gzip;base64,\
H4sIAAAAAAACCmNg+P+fYRQPGgwALacVM5ABAAA=\
");
}
