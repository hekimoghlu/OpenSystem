[Plugin]
Name=TypeScript Language Server
Description=Language server supporting JavaScript and TypeScript
Builtin=true
Embedded=_plugin_ts_language_server_register_types
Module=ts-language-server
X-LSP-Command=typescript-language-server --stdio
X-LSP-Languages=js;jsx;typescript;typescript-jsx
X-LSP-Settings=settings.json
X-Category=lsp
