[Plugin]
Name=PHPize
Description=Use phpize to initialize PHP-based projects
Builtin=true
Embedded=_plugin_phpize_register_types
Module=phpize
X-Category=buildsystem
X-BuildSystem-Name=phpize
X-BuildSystem-Autogen-Command=phpize
X-BuildSystem-Config-Command=$SRCDIR/configure
X-BuildSystem-Build-Command=make
X-BuildSystem-Install-Command=make install
X-BuildSystem-Clean-Command=make clean
