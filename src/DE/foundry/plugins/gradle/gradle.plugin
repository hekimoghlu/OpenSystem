[Plugin]
Name=Gradle
Description=Allow building projects using the gradle build system
Builtin=true
Embedded=_plugin_gradle_register_types
Module=gradle
X-Category=buildsystem
X-BuildSystem-Name=gradle
X-BuildSystem-Autogen-Command=env -C $SRCDIR gradle wrapper
X-BuildSystem-Build-Command=$SRCDIR/gradlew build
X-BuildSystem-Clean-Command=$SRCDIR/gradlew clean
