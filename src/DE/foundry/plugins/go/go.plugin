[Plugin]
Name=Go
Description=Allow building projects using the go build system
Builtin=true
Embedded=_plugin_go_register_types
Module=go
X-Category=buildsystem
X-BuildSystem-Name=go
X-BuildSystem-Downloads-Command=env -C $SRCDIR go get -v
X-BuildSystem-Build-Command=env -C $SRCDIR go build -v
X-BuildSystem-Clean-Command=env -C $SRCDIR go clean -v
