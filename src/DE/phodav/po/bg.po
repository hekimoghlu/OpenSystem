#: bin/chezdav.c:120
msgid "Print program version"
msgstr "Извеждане на версията на програмата"

#: bin/chezdav.c:121
msgid "Be verbose"
msgstr "Извеждане на допълнителна информация"

#: bin/chezdav.c:122
msgid "Port to listen to"
msgstr "Порт за слушане"

#: bin/chezdav.c:123
msgid "Listen on loopback only"
msgstr "Слушане само на локалния интерфейс"

#: bin/chezdav.c:124
msgid "Listen on all interfaces"
msgstr "Слушане на всички интерфейси"

#: bin/chezdav.c:125
msgid "Path to export"
msgstr "Местоположение за изнасяне"

#: bin/chezdav.c:126
msgid "Path to htdigest file"
msgstr "Местоположение на файла htdigest"

#: bin/chezdav.c:127
msgid "DIGEST realm"
msgstr "DIGEST сфера"

#: bin/chezdav.c:128
msgid "Read-only access"
msgstr "Достъп само за четене"

#: bin/chezdav.c:130
msgid "Skip mDNS service announcement"
msgstr "Пропускане на съобщението за mDNS услугата"

#: bin/chezdav.c:142
msgid "- simple WebDAV server"
msgstr "— прост WebDAV сървър"

#: bin/chezdav.c:143
#, c-format
msgid "Report bugs to <%s>"
msgstr "Докладвайте за грешки на <%s>"

#: bin/chezdav.c:150
#, c-format
msgid "Option parsing failed: %s\n"
msgstr "Опцията не може да бъде разчетена: %s\n"

#: bin/chezdav.c:154
#, c-format
msgid "Unsupported extra arguments: %s ...\n"
msgstr "Неподдържани допълнителни аргументи: %s ...\n"

#: bin/chezdav.c:163
msgid "--local and --public are mutually exclusive\n"
msgstr "--local и --public са несъвместим\n"

#: bin/chezdav.c:189
#, c-format
msgid "Failed to open htdigest: %s\n"
msgstr "htdigest не може да бъде отворен: %s\n"

#: bin/chezdav.c:204
#, c-format
msgid "mDNS failed: %s\n"
msgstr "mDNS е неуспешно: %s\n"

#: bin/chezdav.c:215
msgid "Internal error, should not happen\n"
msgstr "Вътрешна грешка, не би трябвало да се случва\n"

#: bin/chezdav.c:218
#, c-format
msgid "Listen failed: %s\n"
msgstr "Слушането е неуспешно: %s\n"
