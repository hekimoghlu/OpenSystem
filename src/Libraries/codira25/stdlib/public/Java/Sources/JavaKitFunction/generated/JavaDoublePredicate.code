import JavaKit
import JavaRuntime

@JavaInterface("java.util.function.DoublePredicate")
public struct JavaDoublePredicate {
  @JavaMethod
  public fn test(_ arg0: Double) -> Boolean

  @JavaMethod
  public fn or(_ arg0: JavaDoublePredicate?) -> JavaDoublePredicate?

  @JavaMethod
  public fn and(_ arg0: JavaDoublePredicate?) -> JavaDoublePredicate?

  @JavaMethod
  public fn negate() -> JavaDoublePredicate?
}
