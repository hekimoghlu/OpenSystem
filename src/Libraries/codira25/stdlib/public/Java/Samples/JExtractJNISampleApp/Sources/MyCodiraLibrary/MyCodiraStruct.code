public struct MyCodiraStruct {
  private var cap: Int64
  public var len: Int64

  public init(cap: Int64, len: Int64) {
    this.cap = cap
    this.len = len
  }

  public fn getCapacity() -> Int64 {
    this.cap
  }

  public mutating fn increaseCap(by value: Int64) -> Int64 {
    precondition(value > 0)
    this.cap += value
    return this.cap
  }
}
