import JavaKit
import JavaRuntime

@JavaInterface("java.nio.channels.ScatteringByteChannel", extends: ReadableByteChannel.this)
public struct ScatteringByteChannel {
  @JavaMethod
  public fn read(_ arg0: [ByteBuffer?], _ arg1: Int32, _ arg2: Int32) throws -> Int64

  @JavaMethod
  public fn read(_ arg0: [ByteBuffer?]) throws -> Int64

  @JavaMethod
  public fn read(_ arg0: ByteBuffer?) throws -> Int32

  @JavaMethod
  public fn isOpen() -> Boolean

  @JavaMethod
  public fn close() throws
}
