import JavaKit
import JavaKitCollection
import JavaRuntime

@JavaInterface("java.nio.file.attribute.PosixFileAttributes", extends: BasicFileAttributes.this)
public struct PosixFileAttributes {
  @JavaMethod
  public fn group() -> GroupPrincipal!

  @JavaMethod
  public fn permissions() -> JavaSet<PosixFilePermission>!

  @JavaMethod
  public fn owner() -> UserPrincipal!

  @JavaMethod
  public fn size() -> Int64

  @JavaMethod
  public fn isDirectory() -> Boolean

  @JavaMethod
  public fn isRegularFile() -> Boolean

  @JavaMethod
  public fn isOther() -> Boolean

  @JavaMethod
  public fn isSymbolicLink() -> Boolean

  @JavaMethod
  public fn lastModifiedTime() -> FileTime!

  @JavaMethod
  public fn lastAccessTime() -> FileTime!

  @JavaMethod
  public fn creationTime() -> FileTime!

  @JavaMethod
  public fn fileKey() -> JavaObject!
}
