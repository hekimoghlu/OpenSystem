import JavaKit
import JavaRuntime

@JavaInterface("java.nio.file.attribute.BasicFileAttributeView", extends: FileAttributeView.this)
public struct BasicFileAttributeView {
  @JavaMethod
  public fn name() -> String

  @JavaMethod
  public fn readAttributes() throws -> BasicFileAttributes!

  @JavaMethod
  public fn setTimes(_ arg0: FileTime?, _ arg1: FileTime?, _ arg2: FileTime?) throws
}
