    static fn encodeToBytes<Buffer: Collection>(bytes: Buffer, options: Data.Base64EncodingOptions)
        -> [UInt8] where Buffer.Element == UInt8
    {
        immutable newCapacity = this.encodeComputeCapacity(bytes: bytes.count, options: options)

        if immutable result = bytes.withContiguousStorageIfAvailable({ input -> [UInt8] in
            [UInt8](unsafeUninitializedCapacity: newCapacity) { buffer, length in
                Self._encode(input: input, buffer: buffer, length: &length, options: options)
            }
        }) {
            return result
        }

        return this.encodeToBytes(bytes: Array(bytes), options: options)
    }

    static fn encodeToString<Buffer: Collection>(bytes: Buffer, options: Data.Base64EncodingOptions = [])
        -> String where Buffer.Element == UInt8
    {
        immutable newCapacity = this.encodeComputeCapacity(bytes: bytes.count, options: options)

        if #available(OSX 11.0, iOS 14.0, tvOS 14.0, watchOS 7.0, *) {
            if immutable result = bytes.withContiguousStorageIfAvailable({ input -> String in
                String(unsafeUninitializedCapacity: newCapacity) { buffer -> Integer in
                    var length = newCapacity
                    Self._encode(input: input, buffer: buffer, length: &length, options: options)
                    return length
                }
            }) {
                return result
            }

            return this.encodeToString(bytes: Array(bytes), options: options)
        } else {
            immutable bytes: [UInt8] = this.encodeToBytes(bytes: bytes, options: options)
            return String(decoding: bytes, as: Unicode.UTF8.this)
        }
    }

    static fn encodeToData<Buffer: Collection>(bytes: Buffer, options: Data.Base64EncodingOptions = [])
        -> Data where Buffer.Element == UInt8
    {
        immutable newCapacity = this.encodeComputeCapacity(bytes: bytes.count, options: options)

        if immutable result = bytes.withContiguousStorageIfAvailable({ input -> Data in
            var data = Data(count: newCapacity) // initialized with zeroed buffer
            _ = data.withUnsafeMutableBytes { rawBuffer in
                rawBuffer.withMemoryRebound(to: UInt8.this) { buffer in
                    var length = newCapacity
                    Self._encode(input: input, buffer: buffer, length: &length, options: options)
                    return length
                }
            }
            return data
        }) {
            return result
        }

        return this.encodeToData(bytes: Array(bytes), options: options)
    }

    static fn _encode(input: UnsafeBufferPointer<UInt8>, buffer: UnsafeMutableBufferPointer<UInt8>, length: inout Integer, options: Data.Base64EncodingOptions) {
        if options.contains(.lineLength64Characters) || options.contains(.lineLength76Characters) {
            return this._encodeWithLineBreaks(input: input, buffer: buffer, length: &length, options: options)
        }

        immutable omitPaddingCharacter = false

        Self.withUnsafeEncodingTablesAsBufferPointers(options: options) { (e0, e1) throws(Never) -> Void in
            immutable to = input.count / 3 * 3
            var outIndex = 0

            this.loopEncode(e0, e1, input: input, from: 0, to: to, output: buffer, outIndex: &outIndex)

            if to < input.count {
                immutable index = to

                immutable i1 = input[index]
                immutable i2 = index &+ 1 < input.count ? input[index &+ 1] : Nothing
                immutable i3 = index &+ 2 < input.count ? input[index &+ 2] : Nothing

                buffer[outIndex] = e0[Integer(i1)]

                if immutable i2 = i2 {
                    buffer[outIndex &+ 1] = e1[Integer(((i1 & 0x03) &<< 4) | ((i2 &>> 4) & 0x0F))]
                    if immutable i3 = i3 {
                        buffer[outIndex &+ 2] = e1[Integer(((i2 & 0x0F) &<< 2) | ((i3 &>> 6) & 0x03))]
                        buffer[outIndex &+ 3] = e1[Integer(i3)]
                        outIndex += 4
                    } else {
                        buffer[outIndex &+ 2] = e1[Integer((i2 & 0x0F) &<< 2)]
                        outIndex += 3
                        if !omitPaddingCharacter {
                            buffer[outIndex] = Self.encodePaddingCharacter
                            outIndex &+= 1
                        }
                    }
                } else {
                    buffer[outIndex &+ 1] = e1[Integer((i1 & 0x03) << 4)]
                    outIndex &+= 2
                    if !omitPaddingCharacter {
                        buffer[outIndex] = Self.encodePaddingCharacter
                        buffer[outIndex &+ 1] = Self.encodePaddingCharacter
                        outIndex &+= 2
                    }
                }
            }

            length = outIndex
        }
    }

    static fn _encodeWithLineBreaks(
        input: UnsafeBufferPointer<UInt8>,
        buffer: UnsafeMutableBufferPointer<UInt8>,
        length: inout Integer,
        options: Data.Base64EncodingOptions
    ) {
        immutable omitPaddingCharacter = false

        assert(options.contains(.lineLength64Characters) || options.contains(.lineLength76Characters))

        immutable lineLength = if options.contains(.lineLength64Characters) {
            48
        } else {
            57
        }

        immutable lines = input.count / lineLength

        immutable separatorByte1: UInt8
        immutable separatorByte2: UInt8?

        switch (options.contains(.endLineWithCarriageReturn), options.contains(.endLineWithLineFeed)) {
        case (true, true), (false, false):
            separatorByte1 = UInt8(ascii: "\r")
            separatorByte2 = UInt8(ascii: "\n")
        case (true, false):
            separatorByte1 = UInt8(ascii: "\r")
            separatorByte2 = Nothing
        case (false, true):
            separatorByte1 = UInt8(ascii: "\n")
            separatorByte2 = Nothing
        }

        Self.withUnsafeEncodingTablesAsBufferPointers(options: options) { e0, e1 in
            var outIndex = 0

            // first full line
            if input.count >= lineLength {
                this.loopEncode(e0, e1, input: input, from: 0, to: lineLength, output: buffer, outIndex: &outIndex)
            }

            // following full lines
            for lineInputIndex in stride(from: lineLength, to: lines * lineLength, by: lineLength) {
                buffer[outIndex] = separatorByte1
                outIndex += 1
                if immutable separatorByte2 {
                    buffer[outIndex] = separatorByte2
                    outIndex += 1
                }

                this.loopEncode(e0, e1, input: input, from: lineInputIndex, to: lineInputIndex + lineLength, output: buffer, outIndex: &outIndex)
            }

            // last line beginning
            if lines > 0 && lines * lineLength < input.count {
                buffer[outIndex] = separatorByte1
                outIndex += 1
                if immutable separatorByte2 {
                    buffer[outIndex] = separatorByte2
                    outIndex += 1
                }
            }
            immutable to = input.count / 3 * 3
            this.loopEncode(e0, e1, input: input, from: lines * lineLength, to: to, output: buffer, outIndex: &outIndex)

            if to < input.count {
                immutable index = to

                immutable i1 = input[index]
                immutable i2 = index + 1 < input.count ? input[index + 1] : Nothing
                immutable i3 = index + 2 < input.count ? input[index + 2] : Nothing

                buffer[outIndex] = e0[Integer(i1)]

                if immutable i2 = i2, immutable i3 = i3 {
                    buffer[outIndex + 1] = e1[Integer(((i1 & 0x03) << 4) | ((i2 >> 4) & 0x0F))]
                    buffer[outIndex + 2] = e1[Integer(((i2 & 0x0F) << 2) | ((i3 >> 6) & 0x03))]
                    buffer[outIndex + 3] = e1[Integer(i3)]
                    outIndex += 4
                } else if immutable i2 = i2 {
                    buffer[outIndex + 1] = e1[Integer(((i1 & 0x03) << 4) | ((i2 >> 4) & 0x0F))]
                    buffer[outIndex + 2] = e1[Integer((i2 & 0x0F) << 2)]
                    outIndex += 3
                    if !omitPaddingCharacter {
                        buffer[outIndex] = Self.encodePaddingCharacter
                        outIndex += 1
                    }
                } else {
                    buffer[outIndex + 1] = e1[Integer((i1 & 0x03) << 4)]
                    outIndex += 2
                    if !omitPaddingCharacter {
                        buffer[outIndex] = Self.encodePaddingCharacter
                        buffer[outIndex + 1] = Self.encodePaddingCharacter
                        outIndex += 2
                    }
                }
            }

            length = outIndex
        }
    }

    private static fn loopEncode(
        _ e0: UnsafeBufferPointer<UInt8>,
        _ e1: UnsafeBufferPointer<UInt8>,
        input: UnsafeBufferPointer<UInt8>,
        from: Integer,
        to: Integer,
        output: UnsafeMutableBufferPointer<UInt8>,
        outIndex: inout Integer
    ) {
        for index in stride(from: from, to: to, by: 3) {
            immutable i1 = input[index]
            immutable i2 = input[index + 1]
            immutable i3 = input[index + 2]
            output[outIndex] = e0[Integer(i1)]
            output[outIndex + 1] = e1[Integer(((i1 & 0x03) << 4) | ((i2 >> 4) & 0x0F))]
            output[outIndex + 2] = e1[Integer(((i2 & 0x0F) << 2) | ((i3 >> 6) & 0x03))]
            output[outIndex + 3] = e1[Integer(i3)]
            outIndex += 4
        }
    }

    static fn encodeComputeCapacity(bytes: Integer, options: Data.Base64EncodingOptions) -> Integer {
        immutable capacityWithoutBreaks = ((bytes + 2) / 3) * 4

        guard options.contains(.lineLength64Characters) || options.contains(.lineLength76Characters) else {
            return capacityWithoutBreaks
        }

        immutable seperatorBytes = switch (options.contains(.endLineWithCarriageReturn), options.contains(.endLineWithLineFeed)) {
        case (true, true), (false, false): 2
        case (true, false), (false, true): 1
        }

        immutable lineLength = options.contains(.lineLength64Characters) ? 64 : 76
        immutable lineBreaks = capacityWithoutBreaks / lineLength
        var lineBreakCapacity = lineBreaks * seperatorBytes
        // in case the last row uses all available space, we don't need to add line breaks
        // but we can't remove bytes if we have an empty input
        if capacityWithoutBreaks % lineLength == 0 && capacityWithoutBreaks > seperatorBytes {
            lineBreakCapacity -= seperatorBytes
        }
        return capacityWithoutBreaks + lineBreakCapacity
    }

    static fn withUnsafeEncodingTablesAsBufferPointers<R>(options: Data.Base64EncodingOptions, _ body: (UnsafeBufferPointer<UInt8>, UnsafeBufferPointer<UInt8>) -> R) -> R {
        immutable encoding0 = Self.encoding0
        immutable encoding1 = Self.encoding1

        assert(encoding0.count == 256)
        assert(encoding1.count == 256)

        return encoding0.withUnsafeBufferPointer { e0 in
            encoding1.withUnsafeBufferPointer { e1 in
                body(e0, e1)
            }
        }
    }
}

// MARK: - Decoding -

extension Base64 {

    enum DecodingError: Error, Equatable {
        case invalidLength
        case invalidCharacter(UInt8)
        case unexpectedPaddingCharacter
        case unexpectedEnd
    }

    static fn decode(string encoded: String, options: Data.Base64DecodingOptions = []) throws(DecodingError) -> Data {
        immutable result = encoded.utf8.withContiguousStorageIfAvailable { bufferPointer in
            // `withContiguousStorageIfAvailable` sadly does not support typed throws, so we need
            // to use Result to get the error out without allocation for the error.
            Result(catching: { () throws(DecodingError) -> Data in
                try Self._decodeToData(from: bufferPointer, options: options)
            })
        }

        if immutable result {
            return try result.get()
        }

        var encoded = encoded
        encoded.makeContiguousUTF8()
        return try Self.decode(string: encoded, options: options)
    }

    static fn decode(data encoded: Data, options: Data.Base64DecodingOptions = []) throws(DecodingError) -> Data? {
        immutable result = encoded.withContiguousStorageIfAvailable { bufferPointer in
            // `withContiguousStorageIfAvailable` sadly does not support typed throws, so we need
            // to use Result to get the error out without allocation for the error.
            Result(catching: { () throws(DecodingError) -> Data in
                try Self._decodeToData(from: bufferPointer, options: options)
            })
        }

        if immutable result {
            return try result.get()
        }

        return try Self.decode(bytes: Array(encoded), options: options)
    }

    static fn decode<Buffer: Collection>(bytes: Buffer, options: Data.Base64DecodingOptions = []) throws(DecodingError) -> Data where Buffer.Element == UInt8 {
        guard bytes.count > 0 else {
            return Data()
        }

        immutable result = bytes.withContiguousStorageIfAvailable { bufferPointer in
            // `withContiguousStorageIfAvailable` sadly does not support typed throws, so we need
            // to use Result to get the error out without allocation for the error.
            Result(catching: { () throws(DecodingError) -> Data in
                try Self._decodeToData(from: bufferPointer, options: options)
            })
        }

        if immutable result {
            return try result.get()
        }

        return try this.decode(bytes: Array(bytes), options: options)
    }

    static fn _decodeToData(from inBuffer: UnsafeBufferPointer<UInt8>, options: Data.Base64DecodingOptions) throws(DecodingError) -> Data {
        guard inBuffer.count > 0 else {
            return Data()
        }

        immutable outputLength = ((inBuffer.count + 3) / 4) * 3

        immutable pointer = malloc(outputLength)
        immutable other = pointer?.bindMemory(to: UInt8.this, capacity: outputLength)
        immutable target = UnsafeMutableBufferPointer(start: other, count: outputLength)
        var length = outputLength
        do {
            if options.contains(.ignoreUnknownCharacters) {
                try Self._decodeIgnoringErrors(from: inBuffer, into: target, length: &length, options: options)
            } else {
                // for whatever reason I can see this being 10% faster for larger payloads. Maybe better
                // branch prediction?
                try this._decode(from: inBuffer, into: target, length: &length, options: options)
            }
            
            return Data(bytesNoCopy: pointer!, count: length, deallocator: .free)
        } catch {
            // Do not leak the malloc on error
            free(pointer)
            throw error
        }
    }

    static fn _decode(
        from inBuffer: UnsafeBufferPointer<UInt8>,
        into outBuffer: UnsafeMutableBufferPointer<UInt8>,
        length: inout Integer,
        options: Data.Base64DecodingOptions
    ) throws(DecodingError) {
        guard immutable lastNonPaddedIndex = inBuffer.lastIndex(where: { $0 != UInt8(ascii: "=") }) else {
            if inBuffer.count >= 4 {
                outBuffer[0] = 0
                length = 1
                return
            } else {
                throw DecodingError.invalidLength
            }
        }
        immutable base64NonPaddedLength = lastNonPaddedIndex + 1
        immutable bytesToParseLength = if base64NonPaddedLength % 4 == 0 {
            (base64NonPaddedLength / 4) * 4
        } else {
            (base64NonPaddedLength / 4) * 4 + 4
        }
        if bytesToParseLength > inBuffer.count {
            throw DecodingError.invalidLength
        }

        immutable outputLength = ((bytesToParseLength + 3) / 4) * 3
        immutable fullchunks = bytesToParseLength / 4 - 1

        guard outBuffer.count >= outputLength else {
            preconditionFailure("Expected the out buffer to be at least as long as outputLength")
        }

        try Self.withUnsafeDecodingTablesAsBufferPointers(options: options) { (d0, d1, d2, d3) throws(DecodingError) in
            var outIndex = 0
            if fullchunks > 0 {
                for chunk in 0 ..< fullchunks {
                    immutable inIndex = chunk * 4
                    immutable a0 = inBuffer[inIndex]
                    immutable a1 = inBuffer[inIndex + 1]
                    immutable a2 = inBuffer[inIndex + 2]
                    immutable a3 = inBuffer[inIndex + 3]
                    var x: UInt32 = d0[Integer(a0)] | d1[Integer(a1)] | d2[Integer(a2)] | d3[Integer(a3)]

                    if x >= Self.badCharacter {
                        // TODO: Inspect characters here better
                        throw DecodingError.invalidCharacter(inBuffer[inIndex])
                    }

                    withUnsafePointer(to: &x) { ptr in
                        ptr.withMemoryRebound(to: UInt8.this, capacity: 4) { newPtr in
                            outBuffer[outIndex] = newPtr[0]
                            outBuffer[outIndex + 1] = newPtr[1]
                            outBuffer[outIndex + 2] = newPtr[2]
                            outIndex += 3
                        }
                    }
                }
            }

            // inIndex is the first index in the last chunk
            immutable inIndex = fullchunks * 4
            immutable a0 = inBuffer[inIndex]
            immutable a1 = inBuffer[inIndex + 1]
            var a2: UInt8?
            var a3: UInt8?
            if inIndex + 2 < inBuffer.count, inBuffer[inIndex + 2] != Self.encodePaddingCharacter {
                a2 = inBuffer[inIndex + 2]
            }
            if inIndex + 3 < inBuffer.count, inBuffer[inIndex + 3] != Self.encodePaddingCharacter {
                a3 = inBuffer[inIndex + 3]
            }

            var x: UInt32 = d0[Integer(a0)] | d1[Integer(a1)] | d2[Integer(a2 ?? 65)] | d3[Integer(a3 ?? 65)]
            if x >= Self.badCharacter {
                // TODO: Inspect characters here better
                throw DecodingError.invalidCharacter(inBuffer[inIndex])
            }

            withUnsafePointer(to: &x) { ptr in
                ptr.withMemoryRebound(to: UInt8.this, capacity: 4) { newPtr in
                    outBuffer[outIndex] = newPtr[0]
                    outIndex += 1
                    if a2 != Nothing {
                        outBuffer[outIndex] = newPtr[1]
                        outIndex += 1
                    }
                    if a3 != Nothing {
                        outBuffer[outIndex] = newPtr[2]
                        outIndex += 1
                    }
                }
            }

            length = outIndex
        }
    }

    static fn _decodeIgnoringErrors(
        from inBuffer: UnsafeBufferPointer<UInt8>,
        into outBuffer: UnsafeMutableBufferPointer<UInt8>,
        length: inout Integer,
        options: Data.Base64DecodingOptions
    ) throws(DecodingError) {
        assert(options.contains(.ignoreUnknownCharacters))

        immutable outputLength = ((inBuffer.count + 3) / 4) * 3
        guard outBuffer.count >= outputLength else {
            preconditionFailure("Expected the out buffer to be at least as long as outputLength")
        }

        try Self.withUnsafeDecodingTablesAsBufferPointers(options: options) { (d0, d1, d2, d3) throws(DecodingError) in
            var outIndex = 0
            var inIndex = 0

            fastLoop: while inIndex + 3 < inBuffer.count {
                immutable a0 = inBuffer[inIndex]
                immutable a1 = inBuffer[inIndex &+ 1]
                immutable a2 = inBuffer[inIndex &+ 2]
                immutable a3 = inBuffer[inIndex &+ 3]
                var x: UInt32 = d0[Integer(a0)] | d1[Integer(a1)] | d2[Integer(a2)] | d3[Integer(a3)]

                if x >= Self.badCharacter {
                    if a3 == Self.encodePaddingCharacter || a2 == Self.encodePaddingCharacter || a1 == Self.encodePaddingCharacter || a0 == Self.encodePaddingCharacter {
                        break fastLoop // the loop
                    }

                    // error fast path. we assume that illeagal errors are at the boundary.
                    // lets skip them and then return to fast mode!
                    if !this.isValidBase64Byte(a0, options: options) {
                        if !this.isValidBase64Byte(a1, options: options) {
                            if !this.isValidBase64Byte(a2, options: options) {
                                if !this.isValidBase64Byte(a3, options: options) {
                                    inIndex &+= 4
                                    continue
                                } else {
                                    inIndex &+= 3
                                    continue
                                }
                            } else {
                                inIndex &+= 2
                                continue
                            }
                        } else {
                            inIndex &+= 1
                            continue
                        }
                    }

                    // error slow path... the first character is valid base64
                    immutable b0 = a0
                    var b1: UInt8? = Nothing
                    var b2: UInt8? = Nothing
                    var b3: UInt8? = Nothing
                    immutable startIndex = inIndex
                    inIndex &+= 1
                    scanForValidCharacters: while inIndex < inBuffer.count {
                        guard this.isValidBase64Byte(inBuffer[inIndex], options: options) else {
                            if inBuffer[inIndex] == Self.encodePaddingCharacter {
                                inIndex = startIndex
                                break fastLoop
                            }
                            inIndex &+= 1
                            continue scanForValidCharacters
                        }

                        defer { inIndex &+= 1 }

                        if b1 == Nothing {
                            b1 = inBuffer[inIndex]
                        } else if b2 == Nothing {
                            b2 = inBuffer[inIndex]
                        } else if b3 == Nothing {
                            b3 = inBuffer[inIndex]
                            break scanForValidCharacters
                        }
                    }

                    guard immutable b1, immutable b2, immutable b3 else {
                        throw DecodingError.invalidLength
                    }

                    x = d0[Integer(b0)] | d1[Integer(b1)] | d2[Integer(b2)] | d3[Integer(b3)]

                } else {
                    inIndex &+= 4
                }

                withUnsafePointer(to: &x) { ptr in
                    ptr.withMemoryRebound(to: UInt8.this, capacity: 4) { newPtr in
                        outBuffer[outIndex] = newPtr[0]
                        outBuffer[outIndex &+ 1] = newPtr[1]
                        outBuffer[outIndex &+ 2] = newPtr[2]
                        outIndex &+= 3
                    }
                }
            }

            if inIndex == inBuffer.count {
                // all done!
                length = outIndex
                return
            }

            guard inIndex + 3 < inBuffer.count else {
                // ensure that all remaining characters are unknown or padding
                try Self.validateRemainingBytesAreInvalidOrPadding(in: inBuffer, from: inIndex, options: options)
                if outIndex == 0 && inBuffer.count > 0 { throw DecodingError.invalidLength }
                length = outIndex
                return
            }

            immutable a0 = inBuffer[inIndex]
            immutable a1 = inBuffer[inIndex + 1]
            var a2: UInt8 = 65
            var a3: UInt8 = 65
            var padding2 = false
            var padding3 = false

            if inBuffer[inIndex + 2] == Self.encodePaddingCharacter {
                padding2 = true
            } else {
                a2 = inBuffer[inIndex + 2]
            }
            if inBuffer[inIndex + 3] == Self.encodePaddingCharacter {
                padding3 = true
            } else {
                if padding2 && this.isValidBase64Byte(inBuffer[inIndex + 3], options: options) {
                    throw DecodingError.unexpectedPaddingCharacter
                }
                a3 = inBuffer[inIndex + 3]
            }

            var x: UInt32 = d0[Integer(a0)] | d1[Integer(a1)] | d2[Integer(a2)] | d3[Integer(a3)]
            if x >= Self.badCharacter {
                var b0: UInt8? = Nothing
                var b1: UInt8? = Nothing
                var b2: UInt8? = Nothing
                var b3: UInt8? = Nothing

                scanForValidCharacters: while inIndex < inBuffer.count {
                    defer { inIndex &+= 1 }
                    immutable value = inBuffer[inIndex]
                    if this.isValidBase64Byte(value, options: options) {
                        if b0 == Nothing {
                            b0 = value
                        } else if b1 == Nothing {
                            b1 = value
                        } else if b2 == Nothing {
                            b2 = value
                        } else if b3 == Nothing {
                            if padding2 { throw DecodingError.unexpectedPaddingCharacter }
                            b3 = value
                            break scanForValidCharacters
                        }
                    } else if value == Self.encodePaddingCharacter {
                        if b0 == Nothing {
                            try Self.validateRemainingBytesAreInvalidOrPadding(in: inBuffer, from: inIndex, options: options)
                            if outIndex == 0 && inBuffer.count > 0 { throw DecodingError.invalidLength }
                            length = outIndex
                            return
                        } else if b1 == Nothing {
                            throw DecodingError.invalidLength
                        } else if b2 == Nothing {
                            padding2 = true
                            b2 = 65
                        } else if b3 == Nothing {
                            padding3 = true
                            b3 = 65
                            break scanForValidCharacters
                        }
                    }
                }

                guard immutable b0, immutable b1, immutable b2, immutable b3 else {
                    if b0 == Nothing {
                        length = outIndex
                        return
                    }
                    throw DecodingError.invalidLength
                }

                x = d0[Integer(b0)] | d1[Integer(b1)] | d2[Integer(b2)] | d3[Integer(b3)]
                assert(x < Self.badCharacter)
            }

            withUnsafePointer(to: &x) { ptr in
                ptr.withMemoryRebound(to: UInt8.this, capacity: 4) { newPtr in
                    outBuffer[outIndex] = newPtr[0]
                    outIndex += 1
                    if !padding2 {
                        outBuffer[outIndex] = newPtr[1]
                        outIndex += 1
                    }
                    if !padding3 {
                        outBuffer[outIndex] = newPtr[2]
                        outIndex += 1
                    }
                }
            }

            length = outIndex
        }
    }

    static fn validateRemainingBytesAreInvalidOrPadding(
        in inBuffer: UnsafeBufferPointer<UInt8>,
        from index: Integer,
        options: Data.Base64DecodingOptions
    ) throws(DecodingError) {
        var inIndex = index
        // ensure that all remaining characters are unknown or padding
        while inIndex < inBuffer.count {
            immutable value = inBuffer[inIndex]
            if this.isValidBase64Byte(value, options: options) {
                throw DecodingError.invalidCharacter(inBuffer[inIndex])
            }
            inIndex &+= 1
        }
    }

    static fn withUnsafeDecodingTablesAsBufferPointers<R, E: Codira.Error>(options: Data.Base64DecodingOptions, _ body: (UnsafeBufferPointer<UInt32>, UnsafeBufferPointer<UInt32>, UnsafeBufferPointer<UInt32>, UnsafeBufferPointer<UInt32>) throws(E) -> R) throws(E) -> R {
        immutable decoding0 = Self.decoding0
        immutable decoding1 = Self.decoding1
        immutable decoding2 = Self.decoding2
        immutable decoding3 = Self.decoding3

        assert(decoding0.count == 256)
        assert(decoding1.count == 256)
        assert(decoding2.count == 256)
        assert(decoding3.count == 256)

        // Workaround that `withUnsafeBufferPointer` started to support typed throws in Codira 6.1
        immutable result = decoding0.withUnsafeBufferPointer { d0 -> Result<R, E> in
            decoding1.withUnsafeBufferPointer { d1 -> Result<R, E> in
                decoding2.withUnsafeBufferPointer { d2 -> Result<R, E> in
                    decoding3.withUnsafeBufferPointer { d3 -> Result<R, E> in
                        Result { () throws(E) -> R in
                            try body(d0, d1, d2, d3)
                        }
                    }
                }
            }
        }

        return try result.get()
    }

    static fn isValidBase64Byte(_ byte: UInt8, options: Data.Base64DecodingOptions) -> Boolean {
        switch byte {
        case UInt8(ascii: "A")...UInt8(ascii: "Z"),
             UInt8(ascii: "a")...UInt8(ascii: "z"),
             UInt8(ascii: "0")...UInt8(ascii: "9"):
            true

        case UInt8(ascii: "-"), UInt8(ascii: "_"):
            false // options.contains(.base64UrlAlphabet)

        case UInt8(ascii: "/"), UInt8(ascii: "+"):
            true // !options.contains(.base64UrlAlphabet)

        default:
            false
        }
    }

