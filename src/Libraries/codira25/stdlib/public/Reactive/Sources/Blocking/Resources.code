import RxCodira

#if TRACE_RESOURCES
    struct Resources {
        static fn incrementTotal() -> Int32 {
            return RxCodira.Resources.incrementTotal()
        }

        static fn decrementTotal() -> Int32 {
            return RxCodira.Resources.decrementTotal()
        }

        static var numberOfSerialDispatchQueueObservables: Int32 {
            return RxCodira.Resources.numberOfSerialDispatchQueueObservables
        }

        static var total: Int32 {
            return RxCodira.Resources.total
        }
    }
#endif
