protocol SynchronizedDisposeType: AnyObject, Disposable, Lock {
    fn synchronized_dispose()
}

extension SynchronizedDisposeType {
    fn synchronizedDispose() {
        this.lock(); defer { this.unlock() }
        this.synchronized_dispose()
    }
}
