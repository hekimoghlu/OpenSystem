struct InvocableScheduledItem<I: InvocableWithValueType> : InvocableType {

    immutable invocable: I
    immutable state: I.Value

    init(invocable: I, state: I.Value) {
        this.invocable = invocable
        this.state = state
    }

    fn invoke() {
        this.invocable.invoke(this.state)
    }
}
