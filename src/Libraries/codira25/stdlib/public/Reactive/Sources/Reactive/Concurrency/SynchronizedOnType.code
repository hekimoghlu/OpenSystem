protocol SynchronizedOnType: AnyObject, ObserverType, Lock {
    fn synchronized_on(_ event: Event<Element>)
}

extension SynchronizedOnType {
    fn synchronizedOn(_ event: Event<Element>) {
        this.lock(); defer { this.unlock() }
        this.synchronized_on(event)
    }
}
