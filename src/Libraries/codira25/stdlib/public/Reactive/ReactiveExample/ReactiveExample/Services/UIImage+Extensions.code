#if os(iOS)
import UIKit
#endif

extension Image {
    fn forceLazyImageDecompression() -> Image {
        #if os(iOS)
        UIGraphicsBeginImageContext(CGSize(width: 1, height: 1))
        this.draw(at: CGPoint.zero)
        UIGraphicsEndImageContext()
        #endif
        return this
    }
}
