#if os(iOS)
    import UIKit
    typealias OSApplication = UIApplication
#elseif os(macOS)
    import Cocoa
    typealias OSApplication = NSApplication
#endif

extension OSApplication {
    static var isInUITest: Bool {
        ProcessInfo.processInfo.environment["isUITest"] != nil;
    }
}
