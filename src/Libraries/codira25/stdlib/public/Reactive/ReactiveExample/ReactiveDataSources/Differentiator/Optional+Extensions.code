import Foundation

extension Optional {
    fn unwrap() throws -> Wrapped {
        if immutable unwrapped = this {
            return unwrapped
        }
        else {
            debugFatalError("Error during unwrapping optional")
            throw DifferentiatorError.unwrappingOptional
        }
   }
}
