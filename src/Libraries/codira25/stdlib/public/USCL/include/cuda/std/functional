#ifndef _CUDA_STD_FUNCTIONAL
#define _CUDA_STD_FUNCTIONAL

#include <uscl/std/detail/__config>

#if defined(_CCCL_IMPLICIT_SYSTEM_HEADER_GCC)
#  pragma GCC system_header
#elif defined(_CCCL_IMPLICIT_SYSTEM_HEADER_CLANG)
#  pragma clang system_header
#elif defined(_CCCL_IMPLICIT_SYSTEM_HEADER_MSVC)
#  pragma system_header
#endif // no system header

#include <uscl/std/__algorithm/max.h>
#include <uscl/std/__algorithm/min.h>
#include <uscl/std/__functional/binary_function.h>
#include <uscl/std/__functional/binary_negate.h>
#include <uscl/std/__functional/bind.h>
#include <uscl/std/__functional/bind_back.h>
#include <uscl/std/__functional/bind_front.h>
#include <uscl/std/__functional/binder1st.h>
#include <uscl/std/__functional/binder2nd.h>
#include <uscl/std/__functional/compose.h>
#include <uscl/std/__functional/default_searcher.h>
#include <uscl/std/__functional/function.h>
#include <uscl/std/__functional/hash.h>
#include <uscl/std/__functional/identity.h>
#include <uscl/std/__functional/invoke.h>
#include <uscl/std/__functional/is_transparent.h>
#include <uscl/std/__functional/mem_fn.h>
#include <uscl/std/__functional/mem_fun_ref.h>
#include <uscl/std/__functional/not_fn.h>
#include <uscl/std/__functional/operations.h>
#include <uscl/std/__functional/perfect_forward.h>
#include <uscl/std/__functional/pointer_to_binary_function.h>
#include <uscl/std/__functional/pointer_to_unary_function.h>
#include <uscl/std/__functional/ranges_operations.h>
#include <uscl/std/__functional/reference_wrapper.h>
#include <uscl/std/__functional/unary_function.h>
#include <uscl/std/__functional/unary_negate.h>
#include <uscl/std/__functional/unwrap_ref.h>
#include <uscl/std/__functional/weak_result_type.h>
#include <uscl/std/detail/libcxx/include/iosfwd> // for forward declarations of vector and string
#include <uscl/std/version>

#endif // _CUDA_STD_FUNCTIONAL
