#ifndef _CUDA_STD_COMPLEX
#define _CUDA_STD_COMPLEX

#include <uscl/std/detail/__config>

#if defined(_CCCL_IMPLICIT_SYSTEM_HEADER_GCC)
#  pragma GCC system_header
#elif defined(_CCCL_IMPLICIT_SYSTEM_HEADER_CLANG)
#  pragma clang system_header
#elif defined(_CCCL_IMPLICIT_SYSTEM_HEADER_MSVC)
#  pragma system_header
#endif // no system header

#include <uscl/std/detail/__config>

#if defined(_CCCL_IMPLICIT_SYSTEM_HEADER_GCC)
#  pragma GCC system_header
#elif defined(_CCCL_IMPLICIT_SYSTEM_HEADER_CLANG)
#  pragma clang system_header
#elif defined(_CCCL_IMPLICIT_SYSTEM_HEADER_MSVC)
#  pragma system_header
#endif // no system header

#include <uscl/std/__complex/arg.h>
#include <uscl/std/__complex/complex.h>
#include <uscl/std/__complex/exponential_functions.h>
#include <uscl/std/__complex/hyperbolic_functions.h>
#include <uscl/std/__complex/inverse_hyperbolic_functions.h>
#include <uscl/std/__complex/inverse_trigonometric_functions.h>
#include <uscl/std/__complex/literals.h>
#include <uscl/std/__complex/logarithms.h>
#include <uscl/std/__complex/math.h>
#include <uscl/std/__complex/nvbf16.h>
#include <uscl/std/__complex/nvfp16.h>
#include <uscl/std/__complex/roots.h>
#include <uscl/std/__complex/trigonometric_functions.h>
#include <uscl/std/__complex/tuple.h>
#include <uscl/std/__complex/vector_support.h>
#include <uscl/std/version>

#endif // _CUDA_STD_COMPLEX
