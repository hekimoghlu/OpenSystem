#ifndef _CUDA_STD_FORMAT
#define _CUDA_STD_FORMAT

#include <uscl/std/detail/__config>

#if defined(_CCCL_IMPLICIT_SYSTEM_HEADER_GCC)
#  pragma GCC system_header
#elif defined(_CCCL_IMPLICIT_SYSTEM_HEADER_CLANG)
#  pragma clang system_header
#elif defined(_CCCL_IMPLICIT_SYSTEM_HEADER_MSVC)
#  pragma system_header
#endif // no system header

#include <uscl/std/__format/buffer.h>
#include <uscl/std/__format/concepts.h>
#include <uscl/std/__format/format_arg.h>
#include <uscl/std/__format/format_arg_store.h>
#include <uscl/std/__format/format_args.h>
#include <uscl/std/__format/format_context.h>
#include <uscl/std/__format/format_error.h>
#include <uscl/std/__format/format_integral.h>
#include <uscl/std/__format/format_parse_context.h>
#include <uscl/std/__format/format_spec_parser.h>
#include <uscl/std/__format/formatter.h>
#include <uscl/std/__format/formatters/bool.h>
#include <uscl/std/__format/formatters/char.h>
#include <uscl/std/__format/formatters/fp.h>
#include <uscl/std/__format/formatters/int.h>
#include <uscl/std/__format/formatters/ptr.h>
#include <uscl/std/__format/formatters/str.h>
#include <uscl/std/__format/output_utils.h>
#include <uscl/std/__format/parse_arg_id.h>
#include <uscl/std/version>

#endif // _CUDA_STD_FORMAT
