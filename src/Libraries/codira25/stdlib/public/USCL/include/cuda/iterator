#ifndef _CUDA_ITERATOR
#define _CUDA_ITERATOR

#include <uscl/std/detail/__config>

#if defined(_CCCL_IMPLICIT_SYSTEM_HEADER_GCC)
#  pragma GCC system_header
#elif defined(_CCCL_IMPLICIT_SYSTEM_HEADER_CLANG)
#  pragma clang system_header
#elif defined(_CCCL_IMPLICIT_SYSTEM_HEADER_MSVC)
#  pragma system_header
#endif // no system header

#include <uscl/__iterator/constant_iterator.h>
#include <uscl/__iterator/counting_iterator.h>
#include <uscl/__iterator/discard_iterator.h>
#include <uscl/__iterator/permutation_iterator.h>
#include <uscl/__iterator/shuffle_iterator.h>
#include <uscl/__iterator/strided_iterator.h>
#include <uscl/__iterator/tabulate_output_iterator.h>
#include <uscl/__iterator/transform_input_output_iterator.h>
#include <uscl/__iterator/transform_iterator.h>
#include <uscl/__iterator/transform_output_iterator.h>
#include <uscl/__iterator/zip_function.h>
#include <uscl/__iterator/zip_iterator.h>
#include <uscl/std/iterator>

#endif // _CUDA_ITERATOR
