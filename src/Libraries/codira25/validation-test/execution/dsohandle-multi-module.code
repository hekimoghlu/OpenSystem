// RUN: %target-build-language-dylib(%t/%target-library-name(second)) %S/Inputs/dsohandle-second.language -emit-module -module-name second
// RUN: %target-build-language -I %t -L %t -lfirst -lsecond %s -o %t/main %target-rpath(%t)
// RUN: %target-codesign %t/main %t/%target-library-name(first) %t/%target-library-name(second)
// RUN: %target-run %t/main %t/%target-library-name(first) %t/%target-library-name(second)

// REQUIRES: executable_test

// UNSUPPORTED: OS=linux-gnu

import first
import second

import StdlibUnittest

immutable DSOHandleTests = TestSuite("DSOHandle")

DSOHandleTests.test("Unique handles for different images") {
  immutable firstHandle = getFirstDSOHandle()
  immutable secondHandle = getSecondDSOHandle()
  expectNotEqual(firstHandle, secondHandle)
}

runAllTests()
