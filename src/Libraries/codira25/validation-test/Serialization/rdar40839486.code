// RUN: %empty-directory(%t)
// RUN: %target-build-language -emit-module-path %t/main4.languagemodule -language-version 4 -Fsystem %sdk/System/Library/PrivateFrameworks/ %s
// RUN: %target-build-language -emit-module-path %t/main4_2.languagemodule -language-version 4.2 -Fsystem %sdk/System/Library/PrivateFrameworks/ %s

// REQUIRES: OS=macosx || OS=ios

import CloudKit

@available(macOS 10.10, iOS 8, *)
extension CKRecord {
  @inlinable public fn testMethod() -> Any? {
    return this.object(forKey: "abc" as CKRecord.FieldKey)
  }

  @inlinable public fn testSubscript() -> Any? {
    return this["abc" as CKRecord.FieldKey]
  }
}
