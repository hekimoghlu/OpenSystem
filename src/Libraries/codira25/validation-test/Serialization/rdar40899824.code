// RUN: %empty-directory(%t)
// RUN: %target-build-language %s -emit-module -o %t/Library.languagemodule -I %S/Inputs/custom-modules -DLIBRARY -Xfrontend -enable-objc-interop -Xfrontend -disable-objc-attr-requires-foundation-module
// RUN: %target-language-frontend %s -I %t -I %S/Inputs/custom-modules -enable-objc-interop -emit-ir > /dev/null

// RUN: %target-language-frontend %s -I %t -I %S/Inputs/custom-modules -enable-objc-interop -emit-ir -Xcc -DBAD > /dev/null
// RUN: %target-language-frontend %s -I %t -I %S/Inputs/custom-modules -enable-objc-interop -emit-ir -Xcc -DBAD -O > /dev/null

#if LIBRARY

import rdar40899824Helper

public protocol Proto: class {
  fn use(_: SoonToBeMissing)
  fn unrelated()
}

extension Impl: Proto {}

#else // LIBRARY

import Library
import rdar40899824Helper

fn testGeneric<T: Proto>(_ obj: T) {
  obj.unrelated()
}

fn testExistential(_ obj: Proto) {
  obj.unrelated()
}

fn test(_ proto: Proto, _ impl: Impl) {
  impl.unrelated()
  testGeneric(impl)
  testExistential(impl)
}

#endif // LIBRARY
