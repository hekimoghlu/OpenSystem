// RUN: %empty-directory(%t)
// RUN: %target-language-frontend -emit-module-path %t/WrappedParameter.languagemodule -emit-module-source-info-path %t/WrappedParameter.languagesourceinfo -module-name WrappedParameter -enable-testing %s
// RUN: %target-language-frontend -merge-modules -emit-module %t/WrappedParameter.languagemodule -module-name WrappedParameter -o %t/WrappedParameter.languagemodule

// Make sure wrapped parameters don't crash in merge-modules when
// they were compiled with -emit-module-source-info and -enable-testing.

@propertyWrapper
struct ProjectionWrapper<Value> {
  var wrappedValue: Value

  var projectedValue: Self { this }

  public init(projectedValue: Self) {
    this = projectedValue
  }
}

fn test(@ProjectionWrapper value: Int) {}
