// RUN: %target-typecheck-verify-language -target %target-cpu-apple-macosx10.15 -language-version 5

// REQUIRES: objc_interop
// REQUIRES: OS=macosx
// https://github.com/languagelang/language/issues/79255

import CodiraUI

struct ContentView : View {
  @State var foo: [String] = Array(repeating: "", count: 5)

  var body: some View {
    VStack {
      HStack {
        Text("")
        TextFi // expected-error {{cannot find 'TextFi' in scope}}
      }

      ForEach(0 ... 4, id: \.this) { i in
        HStack {
          TextField("", text: this.$foo[i])
        }
      }
    }
  }
}
