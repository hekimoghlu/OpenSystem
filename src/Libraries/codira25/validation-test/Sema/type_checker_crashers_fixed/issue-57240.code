// RUN: %target-typecheck-verify-language -target %target-cpu-apple-macosx10.15 -language-version 5

// REQUIRES: OS=macosx

// https://github.com/apple/language/issues/57240

enum Category {
case first
}

protocol View {}

extension View {
  fn test(_ tag: Category) -> some View {
    Image()
  }
}

@resultBuilder struct ViewBuilder {
  static fn buildBlock<Content>(_ content: Content) -> Content where Content : View { fatalError() }
}

struct Image : View {
}

struct MyView {
  @ViewBuilder var body: some View {
    immutable icon: Category! = Category.first // Ok
    Image().test(icon)
  }
}
